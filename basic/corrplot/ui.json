{
  "data": {
    "datTable": {
      "type": "hiplot-textarea",
      "required": true,
      "label": "messages.common.dataTable"
    }
  },
  "extra": {
    "correlation": {
      "type": "select",
      "label": "messages.basic.corHeatmap.correlation",
      "items": [
        "row", "col"
      ],
      "class": "col-12 col-md-4"
    },
    "half": {
      "type": "select",
      "label": "messages.basic.corHeatmap.half",
      "items": [
        "upper", "lower", "full"
      ],
      "class": "col-12 col-md-4"
    },
    "shape": {
      "type": "select",
      "label": "messages.basic.common.shape",
      "items": [
        "circle", "square", "ellipse", "number", "shade", "color", "pie"
      ],
      "class": "col-12 col-md-4"
    },
    "order_method": {
      "type": "select",
      "label": {
        "en": "Order Method",
        "zh_cn": "排序方法"
      },
      "items": [
        "original", "hclust", "AOE", "FPC", "alphabet"
      ],
      "class": "col-12 col-md-4"
    },
    "hc_method": {
      "type": "select",
      "label": "messages.basic.common.hc_method",
      "items": [
        "ward.D",
        "ward.D2",
        "single",
        "complete",
        "average",
        "mcquitty",
        "median",
        "centroid"
      ],
      "class": "col-12 col-md-4"
    },
    "cor_method": {
      "type": "select",
      "label": {
        "en": "Correlation Method",
        "zh_cn": "相关计算方法"
      },
      "items": [
        "pearson", "spearman", "kendall"
      ],
      "class": "col-12 col-md-4"
    },
    "insig": {
      "type": "select",
      "label": "messages.basic.corHeatmap.insig",
      "items": [
        "on", "off"
      ],
      "class": "col-12 col-md-6"
    },
    "insig_type": {
      "type": "select",
      "label": "messages.basic.corHeatmap.insig_type",
      "items": [
        "pch", "p-value", "blank", "n", "label_sig"
      ],
      "class": "col-12 col-md-6"
    },
    "pch_col": {
      "type": "select",
      "label": "messages.basic.corHeatmap.pch_col",
      "items": [
        "black","red","green","blue","purple"
      ],
      "class": "col-12 col-md-6"
    },
    "sig_level": {
      "type": "combobox",
      "label": "messages.basic.common.sig_level",
      "class": "col-12 col-md-6",
      "multiple": true,
      "items": [0.05, 0.01, 0.001, 0.0001]
    },
    "color_low": {
      "type": "color-picker",
      "label": {
        "en": "Color Low",
        "zh_cn": "小值颜色"
      },
      "class": "col-12 col-md-4"
    },
    "color_mid": {
      "type": "color-picker",
      "label": {
        "en": "Color Middle",
        "zh_cn": "中值颜色"
      },
      "class": "col-12 col-md-4"
    },
    "color_high": {
      "type": "color-picker",
      "label": {
        "en": "Color High",
        "zh_cn": "大值颜色"
      },
      "class": "col-12 col-md-4"
    },
    "number_size": {
      "type": "slider",
      "label": {
        "en": "Size of Number",
        "zh_cn": "数字大小"
      },
      "step": 0.001,
      "class": "col-12"
    },
    "pch_size": {
      "type": "slider",
      "label": {
        "en": "Size of Insig",
        "zh_cn": "不显著符号大小"
      },
      "step": 0.001,
      "class": "col-12 col-md-6"
    },
    "pch": {
      "type": "slider",
      "label": {
        "en": "Insig Shape",
        "zh_cn": "不显著形状"
      },
      "max": 25,
      "class": "col-12 col-md-6"
    },
    "lab": {
      "type": "switch",
      "label": {
        "zh_cn": "展示数字",
        "en": "Show Number"
      },
      "class": "col-12 col-md-3"
    },
    "diag": {
      "type": "switch",
      "label": {
        "zh_cn": "对角线",
        "en": "Diagonal"
      },
      "class": "col-12 col-md-3"
    }
  }
}
