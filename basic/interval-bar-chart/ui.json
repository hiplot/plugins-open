{
  "data": {
    "1-data": {
      "type": "hiplot-textarea",
      "label": {
        "en": "Data Table",
        "zh_cn": "数据表"
      },
      "required": true
    }
  },
  "dataArg": {
    "1-data": [
      {
        "label": {
          "en": "min",
          "zh_cn": "min"
        },
        "required": true
      },
      {
        "label": {
          "en": "max",
          "zh_cn": "max"
        },
        "required": true
      },
      {
        "label": {
          "en": "name",
          "zh_cn": "name"
        },
        "required": true
      },
      {
        "label": {
          "en": "mean",
          "zh_cn": "均值"
        },
        "required": false
      }
    ]
  },
  "extra": {
    "x_label": {
      "type": "text-field",
      "label": {
        "en": "X Label",
        "zh_cn": "X轴标签"
      },
      "class": "col-12 col-md-6"
    },
    "y_label ": {
      "type": "text-field",
      "label": {
        "en": "Y Label",
        "zh_cn": "Y轴标签"
      },
      "class": "col-12 col-md-6"
    },
    "bar_color": {
      "type": "color-picker",
      "class": "col-12 col-md-6",
      "label": {
        "en": "Bar Color",
        "zh_cn": "柱状图颜色"
      }
    },
    "line_color": {
      "type": "color-picker",
      "class": "col-12 col-md-6",
      "label": {
        "en": "Mean Line Color",
        "zh_cn": "均值线颜色"
      }
    },
    "line_size": {
      "type": "slider",
      "label": {
        "en": "Mean Line Size",
        "zh_cn": "均值线粗细"
      },
      "step": 0.1,
      "min": 0,
      "max": 5,
      "class": "col-12 col-md-6"
    },
    "bar_width": {
      "type": "slider",
      "label": {
        "en": "Bar Width",
        "zh_cn": "柱状图宽度"
      },
      "step": 0.1,
      "min": 0,
      "max": 1,
      "class": "col-12 col-md-6"
    },
    "need_mean_line": {
      "type": "switch",
      "label": {
        "en": "Draw Mean Line",
        "zh_cn": "绘制均值线"
      },
      "class": "col-12 col-md-6",
      "required": true
    }
  }
}
