{
  "data": {
    "1-countData": {
      "type": "hiplot-textarea",
      "required": true,
      "label": "messages.basic.heatmap.countData"
    },
    "2-sampleInfo": {
      "type": "hiplot-textarea",
      "required": false,
      "label": {
        "en": "SampleInfo",
        "zh_cn": "样本信息"
      }
    }
  },
  "extra": {
    "p_sampling": {
      "type": "slider",
