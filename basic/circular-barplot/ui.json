{
  "data": {
    "datmat": {
      "type": "hiplot-textarea",
      "required": true,
      "label": "messages.common.dataTable"
    }
  },
  "extra": {
    "sort_value": {
      "type": "switch",
      "label": {
        "en": "Sort Value",
        "zh_cn": "按值大小排序"
      },
      "class": "col-12 col-md-4"
    },
    "label": {
      "type": "switch",
      "label": {
        "en": "Label",
        "zh_cn": "标签"
      },
      "class": "col-12 col-md-4"
    },
    "gapbar_num": {
      "type": "slider",
      "label": {
        "en": "Gapbar Num",
        "zh_cn": "间隔柱数目"
      },
      "class": "col-12 col-md-6",
      "max": 5,
      "step": 1
    },
    "label_size": {
      "type": "slider",
      "label": {
        "en": "Label Size",
        "zh_cn": "标签字体大小"
      },
      "class": "col-12 col-md-6",
      "max": 10,
      "step": 0.1
    }
  }
}
