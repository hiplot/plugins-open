{
  "data": {
    "datTable": {
      "type": "hiplot-textarea",
      "required": true,
      "label": "messages.common.dataTable"
    }
  },
  "general": {
    "imageExportType": {
      "plotly": true
    }
  },
  "extra": {
    "add_geom": {
      "type": "select",
      "label": "messages.basic.common.addGeom",
      "items": [
        "boxplot",
        "dotplot",
        "jitter",
        "none"
      ],
      "class": "col-12 col-md-6"
    },
    "stat_method": {
      "type": "select",
      "label": "messages.basic.common.statMethod",
      "items": [
        "t.test",
        "wilcox.test",
        "anova",
        "kruskal.test"
      ],
      "class": "col-12 col-md-6"
    },
    "stat_pos": {
      "type": "slider",
      "label": "messages.basic.common.statPos",
      "class": "col-12",
      "max": 10,
      "min": -10,
      "step": 0.01
    }
  }
}
