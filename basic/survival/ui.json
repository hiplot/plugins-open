{
  "data": {
    "datTable": {
      "type": "hiplot-textarea",
      "required": true,
      "label": "messages.common.dataTable"
    }
  },
  "extra": {
    "risk_table": {
      "type": "switch",
      "label": "messages.basic.survival.risk_table",
      "class": "col-12 col-md-4"
    },
    "risk_table_y_text": {
      "type": "switch",
      "label": "messages.basic.survival.risk_table_y_text",
      "class": "col-12 col-md-4"
    },
    "ncensor_plot": {
      "type": "switch",
      "label": "messages.basic.survival.ncensor_plot",
      "class": "col-12 col-md-4"
    },
    "intervals": {
      "type": "switch",
      "label": "messages.basic.survival.intervals",
      "class": "col-12 col-md-4"
    },
    "pval": {
      "type": "switch",
      "label": "messages.basic.survival.pval",
      "class": "col-12 col-md-4"
    },
    "risk_table_height": {
      "type": "slider",
      "label": "messages.basic.survival.risk_table_height",
      "class": "col-12 col-md-6",
      "min": 0,
      "max": 1,
      "step": 0.01
    },
    "ncensor_plot_height": {
      "type": "slider",
      "label": "messages.basic.survival.ncensor_plot_height",
      "class": "col-12 col-md-6",
      "min": 0,
      "max": 1,
      "step": 0.01
    },
    "line_size": {
      "type": "slider",
      "label": {
        "en": "Line Size",
        "zh_cn": "线条粗细"
      },
      "class": "col-12",
      "step": 0.01
    },
    "surv_median_line": {
      "type": "select",
      "label": "messages.basic.survival.surv_median_line",
      "items": [
        "none", "hv", "h", "v"
      ],
      "class": "col-12 col-md-4"
    },
    "conf_int_style": {
      "type": "select",
      "label": "messages.basic.survival.conf_int_style",
      "items": [
        "ribbon", "step"
      ],
      "class": "col-12 col-md-4"
    },
    "function": {
      "type": "select",
      "label": "messages.basic.survival.function",
      "items": [
        "pct", "event", "cumhaz"
      ],
      "class": "col-12 col-md-4"
    },
    "xlab": {
      "type": "combobox",
      "label": {
        "en": "Time Label",
        "zh_cn": "横轴时间标签"
      },
      "items": ["Time"],
      "class": "col-12 col-md-6"
    },
    "ylab": {
      "type": "combobox",
      "label": {
        "en": "Surv Label",
        "zh_cn": "纵轴生存标签"
      },
      "items": ["Survival probability"],
      "class": "col-12 col-md-6"
    },
    "xlim_start": {
      "type": "slider",
      "label": {
        "en": "X Start",
        "zh_cn": "X轴起始刻度"
      },
      "class": "col-12 col-md-6",
      "max": 1000,
      "min": 0,
      "step": 1
    },
    "xlim_end": {
      "type": "slider",
      "label": {
        "en": "X End",
        "zh_cn": "X轴结束刻度"
      },
      "class": "col-12 col-md-6",
      "min": 0,
      "max": 10000,
      "step": 1
    },
    "ylim_start": {
      "type": "slider",
      "label": {
        "en": "Y Start",
        "zh_cn": "Y轴起始刻度"
      },
      "class": "col-12 col-md-6",
      "max": 100,
      "min": 0,
      "step": 1
    },
    "ylim_end": {
      "type": "slider",
      "label": {
        "en": "Y End",
        "zh_cn": "Y轴结束刻度"
      },
      "class": "col-12 col-md-6",
      "min": 0,
      "max": 100,
      "step": 1
    },
    "break_x": {
      "type": "slider",
      "label": {
        "en": "X Break",
        "zh_cn": "X轴单位刻度"
      },
      "class": "col-12",
      "min": 0,
      "max": 1000,
      "step": 1
    }
  }
}
