{
  "data": {
    "datTable": {
      "type": "hiplot-textarea",
      "required": true,
      "label": "messages.common.dataTable"
    }
  },
  "dataArg": {
    "datTable": [
      {
        "label": "Fusion"
      },
      {
        "label": "Link Width",
        "required": false
      },
      {
        "label": "Link Color",
        "individual": true
      },
      {
        "label": "Label Color"
      }
    ]
  },
  "general": {
    "imageExportType": {
      "ppt": false
    }
  },
  "extra": {
    "label_side": {
      "type": "select",
      "label": {
        "en": "Label Side",
        "zh_cn": "标签位置"
      },
      "items": ["inside", "outside"],
      "class": "col-12"
    },
    "ideogram_height": {
      "type": "slider",
      "label": {
        "en": "Ideogram Height",
        "zh_cn": "染色体高度"
      },
      "step": 0.01,
      "class": "col-12 col-md-6"
    },
    "label_size": {
      "type": "slider",
      "label": {
        "en": "Label Size",
        "zh_cn": "标签大小"
      },
      "step": 0.01,
      "class": "col-12 col-md-6"
    },
    "connection_height": {
      "type": "slider",
      "label": {
        "en": "Height of Connection Track",
        "zh_cn": "连接区高度"
      },
      "step": 0.01,
      "class": "col-12 col-md-6"
    },
    "labels_height": {
      "type": "slider",
      "label": {
        "en": "Label Height",
        "zh_cn": "标签高度"
      },
      "step": 0.01,
      "class": "col-12 col-md-6"
    },
    "show_gene": {
      "type": "switch",
      "label": {
        "en": "Show Gene",
        "zh_cn": "展示基因"
      },
      "class": "col-12 col-md-4"
    },
    "show_link": {
      "type": "switch",
      "label": {
        "en": "Show Link",
        "zh_cn": "展示连线"
      },
      "class": "col-12 col-md-4"
    }
  }
}
