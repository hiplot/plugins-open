{
  "data": {
    "datTable": {
      "type": "hiplot-textarea",
      "required": true,
      "label": "messages.common.dataTable"
    }
  },
  "extra": {
    "heat_eng": {
      "type": "select",
      "label": {
        "en": "Pkg for Heat",
        "zh_cn": "热图包"
      },
      "items": [
        "ComplexHeatmap::pheatmap",
        "ComplexHeatmap::Heatmap",
        "pheatmap::pheatmap",
        "gplots::heatmap.2"
      ],
      "class": "col-12 col-md-6"
    },
    "correlation": {
      "type": "select",
      "label": "messages.basic.corHeatmap.correlation",
      "items": [
        "row",
        "col"
      ],
      "class": "col-12 col-md-6"
    },
    "order_method": {
      "type": "select",
      "label": {
        "en": "Order Method",
        "zh_cn": "排序方法"
      },
      "items": [
        "original",
        "hclust",
        "AOE",
        "FPC",
        "alphabet"
      ],
      "class": "col-12 col-md-6"
    },
    "hc_method": {
      "type": "select",
      "label": "messages.basic.common.hc_method",
      "items": [
        "ward.D",
        "ward.D2",
        "single",
        "complete",
        "average",
        "mcquitty",
        "median",
        "centroid"
      ],
      "class": "col-12 col-md-6"
    },
    "cor_method": {
      "type": "select",
      "label": {
        "en": "Correlation Method",
        "zh_cn": "相关计算方法"
      },
      "items": [
        "pearson",
        "spearman",
        "kendall"
      ],
      "class": "col-12 col-md-4"
    },
    "hc_distance_rows": {
      "type": "select",
      "label": {
        "en": "Row Distance Measure",
        "zh_cn": "行距离度量"
      },
      "items": [
        "euclidean",
        "correlation",
        "maximum",
        "manhattan",
        "canberra",
        "binary",
        "minkowski"
      ],
      "class": "col-12 col-md-4"
    },
    "hc_distance_cols": {
      "type": "select",
      "label": {
        "en": "Column Distance Measure",
        "zh_cn": "列距离度量"
      },
      "items": [
        "euclidean",
        "correlation",
        "maximum",
        "manhattan",
        "canberra",
        "binary",
        "minkowski"
      ],
      "class": "col-12 col-md-4"
    },
    "color_low": {
      "type": "color-picker",
      "label": {
        "en": "Color Low",
        "zh_cn": "小值颜色"
      },
      "class": "col-12 col-md-4"
    },
    "color_mid": {
      "type": "color-picker",
      "label": {
        "en": "Color Middle",
        "zh_cn": "中值颜色"
      },
      "class": "col-12 col-md-4"
    },
    "color_high": {
      "type": "color-picker",
      "label": {
        "en": "Color High",
        "zh_cn": "大值颜色"
      },
      "class": "col-12 col-md-4"
    },
    "out_cor_table": {
      "type": "switch",
      "label": {
        "zh_cn": "输出相关性表格",
        "en": "Output Corr Table"
      },
      "class": "col-12 col-md-6"
    },
    "display_numbers": {
      "type": "switch",
      "label": {
        "en": "Display Value",
        "zh_cn": "显示数值"
      },
      "class": "col-12 col-md-6"
    }
  }
}